#include <bits/stdc++.h>
#define ll long long
using namespace std;
// devisors
vector<ll>Devisors(ll n)
{
    vector<ll> ret;
    for(ll i=1;i*i<=n;i++){
        if(n%i==0)
    {
        ret.push_back(i);
       if(i*i!=n) ret.push_back(n/i);
    }
    }
    return ret;
}


// gcd
ll gcd(ll a, ll b)
{
    if(b>a) swap(a,b);
    if(b==0) return a;
    return gcd(a%b,b);
}



// lcm
ll lcm(ll a,ll b)
{
    return a*b/gcd(a,b);
}


//PrimeNumber
bool isprime(ll n)
{
    if(n==1||n==0) return 0;
    else if(n==2) return 1;
    for(ll i=3;i*i<=n;i+=2)
    {
        if(n%i==0) return 0;
    }
    return 1;
}


// Prime Factorze
vector<pair<ll,ll>> PrimeFactorze(ll n)
{
    vector<pair<ll,ll>>ret;
    for(ll i=2;i*i<=n;i++)
    {
        if(n%i==0)
        {
            int c=0;
            while(n%i==0) n/=i, c++;
            ret.push_back({i,c});
        }
    }
    if(n>1) ret.push_back({n,1});
    return ret;
}



// Power
ll power(ll a,ll b)
{
    if(b==0) return 1;
    else if(b==1) return a;
    ll ret=1;
    if(b&1) ret=a;
    ll x=power(a,b/2);
    return ret*x*x;
}



// Powermod
const int n=1e9+7;
ll powmod(ll a,ll b)
{
    if(b==0) return 1;
    else if(b==1) return a;
    ll ret=1;
    if(b&1) ret=a%mod;
    ll x=powmod(a,b/2);
    return ret*x*x;
}



// Function deal with integers bigger than mod
const ll mod=1e9+7;
// Add
ll add(ll a,ll b)
{
    return ((a%mod)+(b%mod))%mod;
}
// Mul
ll Mul(ll a,ll b)
{
    return ((a%mod)*(b%mod))%mod;
}
// Devide
ll Devide(ll a,ll b)
{
    return Mul(a,powmod(b,mod-2));
}
// Sub
ll Sub(ll a,ll b)
{
    return ((((a%mod)-(b%mod))%mod)+mod)%mod;
}











