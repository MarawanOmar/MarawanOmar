#include <bits/stdc++.h>
using namespace std;
#define ll long long
int countBits(int x) {
    int cnt = 0;
    while (x) {
        cnt += (x & 1);
        x >>= 1;
    }
    return cnt;
}
bool isknown(ll n,int i)
{
    return (n>>i)&1;
}
ll setbit(ll n,int i)
{
    return n|(1<<n);
}
ll resetbit(ll n,int i)
{
    return n&(~(1<<i));
}
ll flip(ll n,int i)
{
    return n^(1<<i);
}
bool ispoweroftwo(int n)
{
    if(n==0) return 0;
    return !(n&(n-1));
}
bool isodd(ll n)
{
   return n&1;
}
int main() {
    ios_base::sync_with_stdio(0);
    cin.tie(0); cout.tie(0);

    return 0;
}
